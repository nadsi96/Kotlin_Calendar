<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="50dp"
        android:height="50dp">
        <rotate android:fromDegrees="90">
            <shape android:shape="line">
                <stroke
                    android:width="5dp"
                    android:color="@color/white"/>
            </shape>
        </rotate>
    </item>

    <item
        android:width="50dp"
        android:height="50dp">
        <rotate>
            <shape android:shape="line">
                <stroke
                    android:width="5dp"
                    android:color="@color/white" />
            </shape>
        </rotate>
    </item>

</layer-list>