<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <rotate android:fromDegrees="0"
            android:toDegrees="45">
            <shape android:shape="line">
                <stroke
                    android:width="5dp"
                    android:color="@color/white"/>
            </shape>
        </rotate>
    </item>

    <item>
        <rotate android:fromDegrees="0"
            android:toDegrees="-45">
            <shape android:shape="line">
                <stroke
                    android:width="5dp"
                    android:color="@color/white" />
            </shape>
        </rotate>
    </item>
</layer-list>